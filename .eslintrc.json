{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "implicit-arrow-linebreak": 0,
    "operator-linebreak": 0,
    "no-unused-expressions": 0,
    "object-curly-newline": 0,
    "function-paren-newline": 0
  }
}
